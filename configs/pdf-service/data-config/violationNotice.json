{
    "key": "violationNotice",
    "DataConfigs": {
      "serviceName": "rainmaker-rp",
      "version": "1.0.0",
      "baseKeyPath": "$.violationNotices.*",
      "entityIdPath":"$.applicationNumber",
      "isCommonTableBorderRequired": true,
      "mappings": [
        {
          "mappings": [
            {
              "direct": [
                {
                  "variable": "application_label",
                  "value": {
                    "path": "APP_NO_LABEL"
                  },
                  "type": "label",
                  "localisation":{
                     "required":true,
                     "prefix":"TL_HOME_SEARCH_RESULTS",   
                     "module":"rainmaker-tl"
                  } 
                },
                {
                  "variable": "municiapl_co",
                  "value": {
                    "path": "LOCALIZATION_ULBGRADE_MC1"
                  },
                  "type": "label",
